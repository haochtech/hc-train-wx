@import "../../../wxParse/wxParse.wxss";

.banner {
    width: 100%;
    height: 320rpx;
}

.times {
    display: flex;
    align-items: center;
    color: #fff;
}

.times .timesL {
    flex: 1;
    background: #7dd6ef;
    padding: 20rpx 30rpx;
}

.times .timesL>view:nth-child(1) {
    font-size: 4.5vw;
    line-height: 6.5vw;
}

.times .timesL>view:nth-child(2) {
    text-decoration: line-through;
}

.times .timesR {
    padding: 20rpx 30rpx;
    text-align: center;
}

.times .timesR>view:nth-child(2) {
    font-size: 4.5vw;
    line-height: 6.5vw;
}

.service {
    background: #fff;
}

.service .title {
    display: flex;
    padding: 20rpx 0;
    align-items: center;
}

.service .title .title_l {
    flex: 1;
    font-size: 4.5vw;
    line-height: 6.5vw;
    padding: 0 30rpx;
}

.service .title .title_r {
    width: 200rpx;
    text-align: center;
    border-left: #f7f7f7 4rpx solid;
}

.service .title .title_r>view:nth-child(1) {
    font-size: 4.5vw;
}

.service .kucun {
    padding: 20rpx 30rpx;
    color: #aaaaaa;
    font-size: 4vw;
}

.service .kucun .sp1 {
    color: #333;
    font-size: 3.5vw;
}

.kanP {
    display: flex;
    padding: 20rpx 30rpx;
    margin-top: 6rpx;
    background: #fff;
}

.kanP .kanPL>image {
    width: 60rpx;
    height: 60rpx;
    border-radius: 100%;
    display: block;
}

.kanP .kanPR {
    flex: 1;
    margin-left: 20rpx;
    font-size: 4vw;
    line-height: 6vw;
}

.kanP .kanPR .pro {
    margin-top: 40rpx;
}

.kanP .kanPR .proB {
    color: #aaa;
    text-align: right;
    position: relative;
}

.kanP .kanPR .proB>view:nth-child(1) {
    position: absolute;
    top: 0;
}

.liu {
    background: #fff;
    padding: 20rpx 30rpx;
    margin-top: 6rpx;
}

.liu .title {
    font-size: 4.5vw;
    line-height: 6.5vw;
}

.liu .liuL {
    display: flex;
    justify-content: center;
    margin-top: 20rpx;
}

.liu .liuL .item {
    text-align: center;
    color: #aaa;
    padding: 0 10rpx;
}

.liu .liuL .item>image {
    width: 70rpx;
    height: 70rpx;
    display: inline-block;
}

.liu .liuL .arrow {
    width: 50rpx;
    margin: 30rpx 0;
}

.list {
    margin-top: 10rpx;
}

.list .nav {
    display: flex;
    background: #fff;
    border-bottom: #ddd 2rpx solid;
}

.list .nav>view {
    flex: 1;
    text-align: center;
    padding: 20rpx 0;
    box-sizing: border-box;
    margin: 0 40rpx;
}

.list .nav>view.curr {
    color: #5fccec;
    border-bottom: #5fccec 4rpx solid;
}

.detail .detail_t {
    background: #fff;
}

.detail .detail_t .item {
    padding: 20rpx 0;
    display: flex;
}

.detail .detail_t .item+.item {
    border-top: #ddd 2rpx solid;
}

.detail .detail_t .item>view:nth-child(1) {
    width: 10rpx;
    height: 40rpx;
    background: #5fccec;
}

.detail .detail_t .item>view:nth-child(2) {
    margin-left: 20rpx;
    color: #666666;
    flex: 1;
}

.detail .detail_t .item .btn {
    width: 120rpx;
    height: 40rpx;
    background: #5fccec;
    color: #fff;
    display: flex;
    align-items: center;
    justify-content: center;
    border-radius: 8rpx;
    margin-right: 20rpx;
}

.detail .detail_c {
    margin-top: 10rpx;
    background: #fff;
}

.detail .detail_c .title {
    padding: 20rpx 0;
    color: #666666;
    display: flex;
}

.detail .detail_c .title>view:nth-child(1) {
    width: 10rpx;
    height: 40rpx;
    background: #5fccec;
    margin-right: 20rpx;
}

.detail .detail_c .content {
    padding-top: 20rpx;
    border-top: #ddd 2rpx solid;
    overflow: hidden;
}

.detail .detail_c .content .item {
    width: 20%;
    float: left;
    text-align: center;
    margin-bottom: 20rpx;
    font-size: 3vw;
}

.detail .detail_c .content .item>image {
    width: 100rpx;
    height: 100rpx;
    border: #ddd 4rpx solid;
    box-sizing: border-box;
    border-radius: 100%;
    margin: 0 auto;
}

.detail .detail_b {
    background: #fff;
    margin-top: 10rpx;
}

.detail .detail_b .title {
    padding: 20rpx 0;
    color: #666666;
    display: flex;
}

.detail .detail_b .title>view:nth-child(1) {
    width: 10rpx;
    height: 40rpx;
    background: #5fccec;
    margin-right: 20rpx;
}

.detail .detail_b .content {
    padding: 20rpx 40rpx;
    border-top: #ddd 2rpx solid;
    color: #999;
}

.cutLog {
}

.cutLog .item {
    display: flex;
    background: #fff;
    padding: 20rpx 30rpx;
    font-size: 4vw;
    line-height: 6vw;
}

.cutLog .item+.item {
    border-top: #ddd 2rpx solid;
}

.cutLog .item .itemL {
    width: 100rpx;
    height: 100rpx;
    border-radius: 100%;
    display: block;
}

.cutLog .item .itemR {
    flex: 1;
    box-sizing: border-box;
    margin-left: 20rpx;
}

.cutLog .item .itemR .itemRT {
    display: flex;
    margin-bottom: 10rpx;
}

.cutLog .item .itemR .itemRT>view:nth-child(1) {
    flex: 1;
}

.cutLog .item .itemR .itemRT>view:nth-child(2) {
    color: #aaa;
}

.footer_block {
    display: none;
}

.footer {
    display: none;
}

.customer_service {
    display: none;
}

.bottom_block {
    height: 120rpx;
}

.bottom {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    background: #fff;
    display: flex;
    border-top: #f7f7f7 4rpx solid;
}

.bottom .icon {
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    flex: 1;
}

.bottom .bottomT {
    flex: 2;
    text-align: center;
    line-height: 100rpx;
    height: 100rpx;
    border-radius: 0;
    border: none;
    border-left: #f7f7f7 4rpx solid;
    font-size: 4.5vw;
    z-index: 99;
    background: #fff;
}

.bottom .bottomT::after {
    display: none;
    content: '';
    border: none;
}

.menu {
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0,0,0,0.5);
    z-index: 1000;
    width: 100%;
    height: 100vh;
    text-align: center;
}

.menu .menu_back {
    width: 80%;
    margin: 20% auto 0;
}

.menu .menu_close {
    margin: 10% auto 0;
}

.menu .menu_box {
    position: absolute;
    top: 40%;
    width: 80%;
    left: 10%;
    color: #fff;
    box-sizing: border-box;
    padding: 20rpx 30rpx;
    font-size: 4.5vw;
    line-height: 6.5vw;
}

.menu .menu_box .menu_btn {
    color: #333;
    width: 200rpx;
    line-height: 70rpx;
    text-align: center;
    background: #fff;
    margin: 20rpx auto 0;
    font-size: 4.5vw;
    padding: 0;
}

.pang {
    margin-top: 6rpx;
    padding: 20rpx;
    background: #fff;
}

.pang .pangB {
    width: 80%;
    margin: 0 auto;
    border-radius: 20rpx;
}

.pang .pangB .title {
    text-align: center;
    padding: 20rpx 0;
    font-size: 4.5vw;
    line-height: 6.5vw;
    color: #999;
}

.pang .pangB .content {
    height: 300rpx;
}

.pang .pangB .content .item {
    display: flex;
    align-items: center;
    padding-top: 14rpx;
    padding-left: 20rpx;
    padding-right: 20rpx;
    color: #b3b3b3;
    font-size: 4vw;
    line-height: 6vw;
}

.pang .pangB .content .itemL {
    width: 70rpx;
    height: 70rpx;
    border-radius: 100%;
    display: block;
}

.pang .pangB .content .itemC {
    padding: 0 20rpx;
    flex: 1;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.pang .pangB .content .itemR {
    position: relative;
    margin: 0 10rpx;
}

.pang .pangB .content .itemR>image {
    width: 60rpx;
    height: 60rpx;
}

.pang .pangB .content .itemR>view {
    position: absolute;
    top: 0;
    left: 0;
    width: 60rpx;
    height: 60rpx;
    text-align: center;
    line-height: 60rpx;
    color: #fff;
}

.pang .pangB .content .itemRR {
    max-width: 140rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}