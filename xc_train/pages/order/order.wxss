.body {
    height: 100%;
    box-sizing: border-box;
    padding: 90rpx 0 101rpx;
}

.bottomBlank2 {
    display: none;
}

.navBlock {
    width: 100%;
    position: fixed;
    left: 0;
    top: 0;
    box-shadow: 0 1rpx 3rpx #999;
    z-index: 50;
    white-space: nowrap;
}

.navBlock .navBox {
    display: inline-block;
    height: 90rpx;
    line-height: 90rpx;
    box-sizing: border-box;
    border-bottom: 2rpx solid transparent;
    font-size: 30rpx;
    text-align: center;
    margin: 0 20rpx;
}

.navBlock .navBox.curr {
    border-color: #77D4C0;
    color: #77D4C0;
}

.orderCont {
    height: 100%;
}

.orderCont swiper {
    height: 100%;
}

.orderCont .listBlock {
    height: 100%;
    box-sizing: border-box;
    padding-bottom: 0.0001px;
}

.orderCont .listBox {
    padding: 0 30rpx;
    margin-bottom: 10rpx;
}

.orderCont .listBox .box0 {
    padding: 30rpx 0;
    border-bottom: 1px solid #e6e6e6;
}

.orderCont .listBox .box0 .text {
    font-size: 26rpx;
    color: #999;
}

.orderCont .listBox .box0 .status {
    font-size: 26rpx;
    color: #ffa64d;
}

.orderCont .listBox .box1 {
    padding: 30rpx 0;
    border-bottom: 1px solid #e6e6e6;
}

.orderCont .listBox .box1 .imgBlock {
    width: 170rpx;
    height: 150rpx;
    margin-right: 15rpx;
    position: relative;
}

.orderCont .listBox .box1 .imgBlock .tag {
    position: absolute;
    width: 54rpx;
    height: 54rpx;
    text-align: center;
    line-height: 46rpx;
    color: #fff;
    font-size: 28rpx;
    border-radius: 0 0 27rpx 27rpx;
    left: 0;
    top: 0;
}

.orderCont .listBox .box1 .imgBlock .tag.bg1 {
    background-color: #FF8000;
}

.orderCont .listBox .box1 .imgBlock .tag.bg2 {
    background-color: #FCC601;
}

.orderCont .listBox .box1 .imgBlock .tag.bg3 {
    background-color: #77D4C0;
}

.orderCont .listBox .box1 .text {
    line-height: 1.2;
    overflow: hidden;
}

.orderCont .listBox .box1 .h1 {
    font-size: 26rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.orderCont .listBox .box1 .h2 {
    font-size: 24rpx;
    color: #999;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    margin-top: 15rpx;
}

.orderCont .listBox .box1 .h2 .sp1 {
    color: #77D3BF;
}

.orderCont .listBox .box1 .h2 .flex-flex1 {
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.orderCont .listBox .box2 {
    padding: 15rpx 0;
}

.orderCont .listBox .box2 .text {
    font-size: 24rpx;
    color: #999;
}

.orderCont .listBox .box2 .text .sp1 {
    color: #77D3BF;
}

.orderCont .listBox .box2 .evas {
    font-size: 22rpx;
    color: #999;
}

.orderCont .listBox .box2 .evas image {
    width: 50rpx;
    height: 50rpx;
    border-radius: 50%;
    margin-right: 8rpx;
}

.orderCont .listBox .box2 .evas .more {
    width: 50rpx;
    height: 50rpx;
    line-height: 50rpx;
    text-align: center;
    border-radius: 50%;
}

.orderCont .listBox .box2 .btn {
    width: 170rpx;
    height: 52rpx;
    line-height: 52rpx;
    border: 1px solid #77D4C0;
    background-color: #77D4C0;
    border-radius: 26rpx;
    font-size: 24rpx;
    color: #fff;
    text-align: center;
    margin-left: 10rpx;
    box-sizing: inherit;
    padding: 0;
}

.orderCont .listBox .box2 .btn::after {
    display: none;
    content: '';
    border-radius: 0;
    border: none;
}

.orderCont .listBox .box2 .btn2 {
    background-color: transparent;
    border-color: #DDDDDD;
    color: #878787;
}

.orderCont .listBox .box2 .disable {
    border-color: #DDDDDD;
    background-color: #ddd;
}

.orderCont .listBox .box3 {
    padding: 50rpx 80rpx;
    position: relative;
    font-size: 24rpx;
    color: #cccccc;
    border-bottom: 1px solid #e6e6e6;
}

.orderCont .listBox .box3 progress {
    border-radius: 4px;
    overflow: hidden;
}

.orderCont .listBox .box3>text {
    position: absolute;
    white-space: nowrap;
}

.orderCont .listBox .box3 .t1 {
    bottom: 20rpx;
    left: 80rpx;
    transform: translateX(-50%);
}

.orderCont .listBox .box3 .t2 {
    bottom: 20rpx;
    right: 80rpx;
    transform: translateX(50%);
}

.orderCont .listBox .box3 .t3 {
    top: 20rpx;
    transform: translateX(-50%);
}

.menu {
    position: fixed;
    width: 350rpx;
    height: 400rpx;
    left: 200rpx;
    top: 300rpx;
    z-index: 2000;
}

.menu .menu_back {
    width: 100%;
    height: 100%;
}

.menu .menux {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    box-sizing: border-box;
    padding: 0 20rpx;
    line-height: 94rpx;
}

.menu .menux .title {
    height: 94rpx;
    border-bottom: #ddd 2rpx dashed;
    text-align: center;
}

.menu .menux .quanma {
    width: 300rpx;
    height: 300rpx;
    display: block;
    margin: 0 auto;
}

.canshow {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100vh;
    background: #000;
    opacity: 0.5;
    z-index: 1200;
}

.flex-display {
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
}

.flex-wrap {
    -webkit-box-flex-wrap: wrap;
    flex-wrap: wrap;
}

.flex-alignC {
    -webkit-box-align: center;
    align-items: center;
    -ms-flex-align: center;
}

.flex-flex1 {
    -webkit-box-flex: 1;
    flex: 1;
}

.flex-flex2 {
    -webkit-box-flex: 2;
    flex: 2;
}

.body image {
    width: 100%;
    height: 100%;
}

.customer_service {
    display: none;
}

.menu2 {
    position: fixed;
    top: 20vw;
    width: 90vw;
    left: 5vw;
    z-index: 9999;
    background: #fff;
    border-radius: 40rpx;
    box-sizing: border-box;
    padding: 40rpx;
}

.menu2 .menu_close {
    position: absolute;
    top: 20rpx;
    right: 20rpx;
}

.menu2 .menu_item {
    padding: 20rpx 0;
}

.menu2 .menu_item textarea {
    border: #ddd 2rpx solid;
    margin-top: 20rpx;
    height: 200rpx;
}

.menu2 .menu_btn {
    color: #fff;
    height: 80rpx;
    font-size: 4.5vw;
    display: flex;
    justify-content: center;
    align-items: center;
}