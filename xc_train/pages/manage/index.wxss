.top {
    background: #f15789;
    text-align: center;
    padding-top: 30rpx;
    box-sizing: border-box;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    z-index: 10;
}

.top .avatar {
    width: 150rpx;
    height: 150rpx;
    border-radius: 100%;
    border: #bfebf7 4rpx solid;
    margin: 0 auto;
}

.top .nick {
    color: #fff;
    font-size: 4vw;
    margin-top: 20rpx;
    height: 40rpx;
}

.top .search {
    display: flex;
    margin-top: 20rpx;
    padding: 20rpx 30rpx;
}

.top .search>image {
    width: 60rpx;
    height: 60rpx;
}

.top .search>input {
    background: #fff;
    flex: 1;
    border-radius: 10rpx;
    margin: 0 20rpx;
    line-height: 60rpx;
    min-height: 60rpx;
}

.top .search>icon {
    display: flex;
    align-items: center;
    justify-content: center;
}

.nav {
    background: #fff;
    display: flex;
    width: 100%;
    font-size: 4vw;
}

.nav>view {
    flex: 1;
    display: flex;
    align-items: center;
    justify-content: center;
    margin: 0 20rpx;
    height: 90rpx;
}

.nav>view.curr {
    border-bottom: #5fcceb 4rpx solid;
}

.nav_block {
    height: 460rpx;
}

.list {
    margin-top: 10rpx;
    background: #fff;
}

.list .item {
    padding: 20rpx 30rpx 20rpx 60rpx;
    display: flex;
    align-items: center;
    position: relative;
}

.list .item .item_btn {
    position: absolute;
    height: 100%;
    top: 0;
    left: 0;
    background: #5fcceb;
    display: flex;
    align-items: center;
    justify-content: center;
    width: 40rpx;
    padding: 10rpx;
    box-sizing: border-box;
    line-height: 4vw;
    font-size: 3vw;
    color: #fff;
}

.list .item+.item {
    border-top: #ddd 2rpx solid;
}

.list .item .item_l {
    flex: 1;
    font-size: 3vw;
    width: 300rpx;
}

.list .item .item_l>view {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.list .item .item_l text {
    color: #B7B7B7;
}

.list .item .item_c {
    display: flex;
    align-items: center;
    flex: 1;
}

.list .item .item_c>image {
    width: 80rpx;
    height: 80rpx;
    background: #000;
    border-radius: 100%;
}

.list .item .item_c>view {
    margin-left: 10rpx;
    width: 210rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.list .item .item_r {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.list .list_nav {
    white-space: nowrap;
}

.list .list_nav view {
    padding: 20rpx 0;
    display: inline-block;
    width: 12%;
    text-align: center;
    margin: 0 4%;
}

.list .list_c {
    border-top: #f7f7f7 10rpx solid;
}

.list .list_c .list_item {
    display: flex;
    padding: 30rpx 20rpx;
}

.list .list_c .list_item+.list_item {
    border-top: #f7f7f7 4rpx solid;
}

.list .list_c .list_item>view:nth-child(1) {
    flex: 1;
}

.list .list_c .list_item>view:nth-child(2) {
    color: #999;
}

.list .list_c .list_item .member {
    color: #333;
}

.menu {
    position: fixed;
    top: 20vw;
    width: 80vw;
    left: 10vw;
    background: #fff;
    z-index: 1000;
    border-radius: 20rpx;
    box-sizing: border-box;
    padding: 40rpx 80rpx;
}

.menu .title {
    text-align: center;
    font-size: 4.5vw;
}

.menu .menu_list {
    margin-top: 40rpx;
}

.menu .menu_list>view {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.menu .menu_list view+view {
    margin-top: 10rpx;
}

.menu .menu_btn {
    width: 100%;
    text-align: center;
    background: #5fcceb;
    line-height: 70rpx;
    height: 70rpx;
    border-radius: 10rpx;
    color: #fff;
    margin-top: 50rpx;
}

.menu .menu_prize {
    margin-top: 40rpx;
}

.menu .menu_prize>image {
    width: 180rpx;
    height: 180rpx;
    margin: 0 auto;
    background: #000;
}

.menu .menu_prize .name {
    margin: 20rpx 0;
    text-align: center;
}

.menu2 {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    background: #fff;
    z-index: 1000;
    box-sizing: border-box;
}

.menu2 .title {
    padding: 20rpx 30rpx;
    display: flex;
    font-size: 4.5vw;
}

.menu2 .title>view:nth-child(1) {
    flex: 1;
}

.menu2 .title>view:nth-child(2) {
    color: #999;
}

.menu2 .title .member {
    color: #333;
}

.menu2 .content {
    max-height: 40vh;
    padding: 0 3vw;
    overflow: scroll;
    margin-top: 20rpx;
}

.menu2 .content>view {
    width: 29.3333333333333333%;
    border: #999 2rpx solid;
    border-radius: 10rpx;
    text-align: center;
    line-height: 60rpx;
    margin-left: 1.5vw;
    margin-right: 1.5vw;
    margin-bottom: 3vw;
    float: left;
    color: #999999;
    display: flex;
    justify-content: center;
}

.menu2 .content .nick {
    max-width: 100rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.menu2 .btn {
    width: 100%;
    text-align: center;
    padding: 20rpx 0;
    color: #fff;
    font-size: 4.5vw;
    margin-top: 40rpx;
}