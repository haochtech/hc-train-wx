.top {
    position: relative;
}

.top>image {
    width: 100%;
}

.top .name {
    height: 100rpx;
    width: 710rpx;
    background: #17a2ca;
    color: #fff;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 5vw;
    border-radius: 20rpx;
    position: absolute;
    left: 50%;
    bottom: -15%;
    transform: translate(-50%);
}

.imgs {
    margin-top: 60rpx;
    padding: 20rpx 20rpx 0;
    overflow: hidden;
}

.imgs.two .item {
    width: 347rpx;
    height: 296rpx;
    float: left;
    position: relative;
    margin-bottom: 20rpx;
}

.imgs.three .item {
    width: 226rpx;
    height: 289rpx;
    float: left;
    position: relative;
    margin-bottom: 20rpx;
}

.imgs.four .item {
    width: 165rpx;
    height: 281rpx;
    float: left;
    position: relative;
    margin-bottom: 20rpx;
}

.imgs .item:nth-child(3n),.imgs .item:nth-child(3n-1) {
    margin-left: 16rpx;
}

.imgs.four .item:nth-child(4n),.imgs.four .item:nth-child(4n-1),.imgs.four .item:nth-child(4n-2) {
    margin-left: 16rpx;
}

.imgs .item .item_t {
    width: 100%;
    height: 100%;
    border-radius: 10rpx;
}

.imgs .item .item_b {
    position: absolute;
    top: 0;
    left: 0;
    background-color: rgba(0,0,0,0.5);
    width: 100%;
    height: 100%;
    border-radius: 10rpx;
    padding: 0;
    font-size: 3.5vw;
    margin: 0;
}

.imgs .item .item_b>view:nth-child(1) {
    width: 120rpx;
    height: 120rpx;
    text-align: center;
    line-height: 120rpx;
    font-size: 8vw;
    background: #000;
    border-radius: 100%;
    margin: 0 auto;
    color: #ffc926;
    position: absolute;
    left: 50%;
    top: 20%;
    transform: translate(-50%);
}

.imgs .item .item_b>view:nth-child(2) {
    width: 100%;
    background: #000;
    color: #ffc926;
    position: absolute;
    bottom: 0;
    left: 0;
    border-radius: 0 0 10rpx 10rpx;
    height: 50rpx;
    line-height: 50rpx;
    text-align: center;
}

.page .list {
    margin-top: 10rpx;
    background: #fff;
}

.page .list .title {
    background: #fff;
    height: 80rpx;
    position: relative;
}

.page .list .title .line {
    height: 1rpx;
    background: #ddd;
    width: 360rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
}

.page .list .title .text {
    width: 180rpx;
    background: #fff;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    text-align: center;
    color: #17a2ca;
    font-size: 4.5vw;
}

.page .list .content {
    padding: 20rpx;
    color: #777777;
    display: block;
}

.page .schedule {
    padding: 20rpx 30rpx;
    background: #fff;
}

.page .schedule .time {
    color: #ee4848;
}

.page .schedule .item_h {
    padding: 0 30rpx;
}

.page .schedule .line {
    width: 100%;
    height: 40rpx;
    background: #eeeeee;
    border-radius: 40rpx;
    margin-top: 20rpx;
    position: relative;
}

.page .schedule .line .line_on {
    position: absolute;
    top: 0;
    left: 0;
    height: 40rpx;
    border-radius: 40rpx;
}

.page .schedule .line .line_t {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    color: #fff;
}

.page .schedule .imgs {
    display: flex;
    margin-top: 20rpx;
    padding: 0;
    padding-bottom: 10rpx;
}

.page .schedule .imgs image+image {
    margin-left: 13rpx;
}

.page .schedule .imgs>image {
    border-radius: 100%;
}

.page .schedule .imgs>view {
    width: 50rpx;
    height: 50rpx;
    border-radius: 100%;
    box-sizing: border-box;
    border: #999 2rpx solid;
    text-align: center;
    line-height: 46rpx;
    font-size: 3vw;
    color: #999;
    margin-left: 13rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.menu {
    position: fixed;
    top: 20vw;
    width: 80vw;
    left: 10vw;
    background: #fff;
    z-index: 1000;
    border-radius: 20rpx;
    box-sizing: border-box;
    padding: 40rpx 80rpx;
}

.menu .title {
    text-align: center;
    font-size: 4.5vw;
}

.menu .menu_btn {
    width: 100%;
    text-align: center;
    background: #5fcceb;
    line-height: 70rpx;
    height: 70rpx;
    border-radius: 70rpx;
    color: #fff;
    margin-top: 50rpx;
}

.menu .menu_prize {
    margin-top: 40rpx;
}

.menu .menu_prize>image {
    width: 180rpx;
    height: 180rpx;
    margin: 0 auto;
    background: #000;
}

.menu .menu_prize .name {
    margin: 20rpx 0;
    text-align: center;
}

.imgs.gua {
    position: relative;
    padding: 0;
    margin: 80rpx 20rpx 20rpx;
}

.imgs .g_img {
    width: 100%;
    height: 300rpx;
}

.imgs .gua {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.5);
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.imgs .gua>view:nth-child(1) {
    color: #fff;
    font-size: 5vw;
    text-align: center;
}

.imgs .gua>view:nth-child(2) {
    background: #aaaaaa;
    width: 200rpx;
    text-align: center;
    line-height: 50rpx;
    border-radius: 50rpx;
    margin: 0 auto;
    color: #d0d0d0;
    margin-top: 20rpx;
    font-size: 3.5vw;
}