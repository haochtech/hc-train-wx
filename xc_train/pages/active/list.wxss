.page .item {
    background: #fff;
}

.page .item+.item {
    margin-top: 10rpx;
}

.page .item>image {
    width: 100%;
    height: 320rpx;
}

.page .item .item_b {
    padding: 0 30rpx;
}

.page .item .item_b .item_t {
    padding: 10rpx 0;
    display: flex;
}

.page .item .item_b .item_t>view:nth-child(1) {
    flex: 1;
    margin-right: 10rpx;
}

.page .item .item_b .item_h {
    padding: 10rpx 30rpx;
    border-top: #ddd 2rpx solid;
}

.page .item .item_b .item_h>view:nth-child(1) {
    text-align: center;
    color: #999999;
}

.page .item .item_b .item_h .line {
    width: 100%;
    height: 40rpx;
    background: #eeeeee;
    border-radius: 40rpx;
    margin-top: 20rpx;
    position: relative;
}

.page .item .item_b .item_h .line .line_on {
    position: absolute;
    top: 0;
    left: 0;
    height: 40rpx;
    border-radius: 40rpx;
}

.page .item .item_b .item_h .line .line_t {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    color: #fff;
}

.page .item .item_b .item_h .imgs {
    display: flex;
    margin-top: 20rpx;
    padding-bottom: 10rpx;
}

.page .item .item_b .item_h .imgs>image {
    border-radius: 100%;
}

.page .item .item_b .item_h .imgs image+image {
    margin-left: 13rpx;
}

.page .item .item_b .item_h .imgs>view {
    width: 50rpx;
    height: 50rpx;
    border-radius: 100%;
    box-sizing: border-box;
    border: #999 2rpx solid;
    text-align: center;
    line-height: 46rpx;
    font-size: 3vw;
    color: #999;
    margin-left: 13rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.menu {
    position: fixed;
    top: 20vw;
    width: 80vw;
    left: 10vw;
    background: #fff;
    z-index: 1000;
    border-radius: 20rpx;
    box-sizing: border-box;
    padding: 40rpx 80rpx;
}

.menu .title {
    text-align: center;
    font-size: 4.5vw;
}

.menu .menu_btn {
    width: 100%;
    text-align: center;
    background: #5fcceb;
    line-height: 70rpx;
    height: 70rpx;
    border-radius: 70rpx;
    color: #fff;
    margin-top: 50rpx;
}

.menu .menu_prize {
    margin-top: 40rpx;
}

.menu .menu_prize>image {
    width: 180rpx;
    height: 180rpx;
    margin: 0 auto;
    background: #000;
}

.menu .menu_prize .name {
    margin: 20rpx 0;
    text-align: center;
}