.nav {
    display: flex;
    text-align: center;
    background: #fff;
}

.nav>view {
    flex: 1;
    padding: 20rpx;
}

.nav>view.curr {
    color: #ff4c60;
    border-bottom: #ff4c60 4rpx solid;
}

.content {
    background: #fff;
    padding: 20rpx 20rpx 0;
    margin-top: 20rpx;
}

.status1 {
    background: url('data:image/png;base64,') no-repeat;
}

.status2 {
    background: url('data:image/png;base64,') no-repeat;
}

.status3 {
    background: url('data:image/png;base64,') no-repeat;
}

.content .item {
    width: 100%;
    height: 23.466666666666vw;
    background-size: 100% 100%;
    margin-bottom: 20rpx;
    display: flex;
    padding: 20rpx;
    color: #fff;
    align-items: center;
    box-sizing: border-box;
    position: relative;
}

.content .item .item_hd {
    padding: 0 20rpx;
    font-size: 3vw;
}

.content .item .item_hd>view:nth-child(1) {
    font-size: 4vw;
}

.content .item .item_hd .price {
    font-size: 8vw;
}

.content .item .item_bd {
    padding: 0 20rpx;
    border-left: #fff 2rpx solid;
}

.content .item .item_bd>view:nth-child(2) {
    font-size: 3vw;
}

.content .item .item_btn {
    width: 150rpx;
    height: 150rpx;
    background: #fff;
    border-radius: 100%;
    color: #ff4c60;
    box-sizing: border-box;
    text-align: center;
    display: flex;
    flex-direction: column;
    justify-content: center;
    border: #a5e2f4 8rpx solid;
    position: absolute;
    top: 50%;
    right: 20rpx;
    transform: translate(0,-50%);
}

.content .item .item_btn.curr {
    border: #bcbcbc 8rpx solid;
    color: #444444;
}

.content .item .item_btn.curr2 {
    border: #d9d9d9 8rpx solid;
    color: #444444;
}