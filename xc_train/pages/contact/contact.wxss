.page .top {
    width: 100%;
}

.list {
    background: #fff;
    padding: 0 30rpx;
    margin-top: 10rpx;
    color: #777;
}

.list .item {
    padding: 20rpx 0;
    display: flex;
    align-items: center;
}

.list .item>image {
    margin-right: 10rpx;
}

.list .item+.item {
    border-top: #ddd 2rpx solid;
}