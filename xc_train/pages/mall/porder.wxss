.flex-display {
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
}

.flex-wrap {
    -webkit-box-flex-wrap: wrap;
    flex-wrap: wrap;
}

.flex-alignC {
    -webkit-box-align: center;
    align-items: center;
    -ms-flex-align: center;
}

.flex-flex1 {
    -webkit-box-flex: 1;
    flex: 1;
}

.flex-flex2 {
    -webkit-box-flex: 2;
    flex: 2;
}

.store_page {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100vh;
    background: #f7f7f7;
    z-index: 100;
    overflow: scroll;
}

.store_page .item+.item {
    margin-top: 10rpx;
}

.store_page .item {
    background: #fff;
}

.store_page .item .item_t {
    display: flex;
    padding: 20rpx 30rpx;
    background: #fff;
    border-radius: 20rpx 20rpx 0 0;
}

.store_page .item .item_t>image {
    width: 100rpx;
    height: 100rpx;
    border-radius: 100%;
}

.store_page .item .item_t .item_l {
    flex: 1;
    width: 450rpx;
    box-sizing: border-box;
    padding-left: 20rpx;
}

.store_page .item .item_t .item_l>view {
    width: 450rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #999;
    line-height: 4vw;
}

.store_page .item .item_t .item_l>view:nth-child(1) {
    color: #333;
    font-size: 4vw;
    margin-bottom: 10rpx;
}

.store_page .item .item_t .item_r {
    display: flex;
    align-items: center;
    flex-direction: column;
    justify-content: center;
    line-height: 4vw;
    color: #999;
    border-left: #f3a7bf 2rpx dashed;
    padding-left: 20rpx;
    width: 100rpx;
    box-sizing: border-box;
}

.pei {
    display: flex;
